{
    "manifest_version": 2,
    "name": "noAIsearch",
    "version": "1.0",
  
    "description": "Adds \"no AI\" tags to searches.",
  
    "icons": {
      "48": "icons/noAIsearch.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.google.com/*", "*://*.duckduckgo.com/*", "*://*.bing.com/*"],
        "js": ["noAIsearch.js"]
      }
    ]
  }
  